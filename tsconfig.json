{
    "compilerOptions": {
      "target": "ES5",
      "outDir": "./dist",
      "strict": true,
      "noImplicitAny": false,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
          "@mattermost/types/*": ["node_modules/@mattermost/types/lib/*"]
      },
      "types": [
        "node"
      ],
      "skipLibCheck": true,
      "esModuleInterop": true
    },
    "include": [
      "./src",
      // "node_modules/@types/**/*"
    ]
}
